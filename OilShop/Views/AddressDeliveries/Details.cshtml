@model OilShop.Models.Data.AddressDelivery

@{
    ViewData["Title"] = "Адрес доставки";
}

<h3>
    <a class="btn text-dark" asp-action="Index" title="Вернуться к списку">
        <i class="fas fa-arrow-left"></i>
    </a>
    Просмотр адреса доставки
</h3>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Street)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Street)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.House)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.House)
        </dd>
    </dl>
</div>
